{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "object-curly-spacing": "off",
    "linebreak-style": 0,
    "isolatedModules": 0,
    "arrow-parens": "off"

  }
}
